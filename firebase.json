{
  "hosting": {
    "public": "./app/dist/ecau",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*/*.mp4",
      "**/*.mp4",
      "*.mp4",
      "**/*/*.m4a",
      "**/*.m4a",
      "*.m4a"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
